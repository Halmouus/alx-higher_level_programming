	<script type="text/javascript">
		var attrValue = "hidden";
		$('#codeRC').css("visibility", "hidden");
		$('#labelJuridiction').css("visibility", "hidden");
		
		
		$('#resultBox').css("visibility", attrValue);
		$('#rcRadio').on( 'change', function () {
			$('#codeRC').css("visibility", "visible");
			$('#labelJuridiction').css("visibility", "visible");
			$('#mCriteria').attr("placeholder","RC");
			$('#mCriteria').val("");
		});
		if ($('#rcRadio').attr("checked")) {
			$('#codeRC').css("visibility", "visible");
			$('#labelJuridiction').css("visibility", "visible");
			$('#mCriteria').attr("placeholder","RC");
		}
		$('#iceRadio').on( 'change', function () {
			$('#codeRC').css("visibility", "hidden");
			$('#labelJuridiction').css("visibility", "hidden");
			$('#mCriteria').attr("placeholder","ICE");
			$('#mCriteria').val("");
		});
		$('#ifRadio').on( 'change', function () {
			$('#labelJuridiction').css("visibility", "hidden");
			$('#codeRC').css("visibility", "hidden");
			$('#mCriteria').attr("placeholder","IF");
			$('#mCriteria').val("");
		});
		
	</script>
